    <version>0.5.0-SNAPSHOT</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>

  <artifactId>joala-labs</artifactId>
  <name>Joala Labs</name>
  <description><![CDATA[
    Joala Labs is a module which provides experimental features. These experimental features
